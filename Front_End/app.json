{
  "expo": {
    "name": "PausePlus",
    "slug": "PausePlus",
    "scheme": "pauseplus",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/app-icon.png",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/app-icon.png",
        "backgroundColor": "transparent"
      },
      "permissions": ["android.permission.RECEIVE_BOOT_COMPLETED"],
      "package": "com.wolfwiz12.pauseplus"
    },
    "web": {
      "bundler": "metro",
      "output": "server",
      "favicon": "./assets/images/app-icon.png"
    },
    "plugins": [
      [
        "expo-router",
        {
          "origin": "https://pauseplus.click/"
        }
      ],
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "enableFullScreenImage_legacy": true
        }
      ],
      [
        "expo-font",
        {
          "fonts": [
            "./assets/fonts/Rubik-Bold.ttf",
            "./assets/fonts/Rubik-ExtraBold.ttf",
            "./assets/fonts/Rubik-Light.ttf",
            "./assets/fonts/Rubik-Medium.ttf",
            "./assets/fonts/Rubik-Regular.ttf",
            "./assets/fonts/Rubik-SemiBold.ttf",
            "./assets/fonts/Charm-Regular.ttf",
            "./assets/fonts/Charm-Bold.ttf",
            "./assets/fonts/LibreCaslonText-Regular.ttf",
            "./assets/fonts/LibreCaslonText-Bold.ttf",
            "./assets/fonts/LibreCaslonText-Italic.ttf",
            "./assets/fonts/ZTGatha-SemiBold.ttf"
          ]
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },

    "extra": {
      "oneSignalAppId": "<1902ad14-5703-46af-8df8-e2a291f67ab6>"
    }
  }
}
